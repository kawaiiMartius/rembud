<%= form_for @supplier, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :qualification, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :qualification, :class => 'number_field' %>
    </div>
  </div>
  
  <%= render partial: 'resoursable/form', object: f %>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                suppliers_path, :class => 'btn' %>
  </div>
<% end %>
